import os

Import('qttbenv')

# uic ScopePlotTest form
qttbenv.Uic4('ScopePlotTest.ui')

# build scopeplottest
scopeplottest = qttbenv.Program(
   'scopeplottest',
   ['main.cpp','ScopePlotTest.cpp','ScopePoke.cpp'],
   LIBS=['QtCore','QtGui','scopeplot','qwt'],
   LIBPATH=['../','$QWTDIR/lib','$QTDIR/lib']
)

qttbenv.InstallBin(scopeplottest)

Default(scopeplottest)
