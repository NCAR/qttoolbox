
Import('env')
env = env.Create("qttoolbox.twoknobstest", tools = ['default', 'qt'])
#env.Append(CCFLAGS = " -I Knob -I TwoKnobs")
#env.Append(LIBS=['twoknobs','knob'])
#env.Append(LIBPATH=['#/Knob', '#/TwoKnobs'])
env.Require(Split("""
PKG_QWT
PKG_QT
PKG_QTTOOLBOX_TWOKNOBS
"""))

p = env.Program('twoknobstest',Split("""
  TwoKnobsTest.ui
  main.cpp
  """) )

Default(p)
