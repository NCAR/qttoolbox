import os

Import('env')

# uic knobtest form
env.Uic4('TwoKnobsTest.ui')

# build knobtest
twoknobstest = env.Program(
   'twoknobstest',
   'main.cpp',
   LIBS=['QtCore','QtGui','twoknobs','qwt'],
   LIBPATH=['../','$QWTDIR/lib','$QTDIR/lib']
)

Default(twoknobstest)
 
env.InstallBin(twoknobstest)
