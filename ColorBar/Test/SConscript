import os

Import('env')

# uic colorbartest form
env.Uic4('ColorBarTest.ui')

# build colorbartest
colorbartest = env.Program(
   'colorbartest',
   ['main.cpp','ColorBarTest.cpp'],
   LIBS=['QtCore','QtGui','colorbar'],
   LIBPATH=['../','$QWTDIR/lib','$QTDIR/lib']
)

Default(colorbartest)

env.InstallBin(colorbartest)
